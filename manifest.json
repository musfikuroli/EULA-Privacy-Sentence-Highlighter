{
  "manifest_version": 3,
  "name": "EULA Analyzer",
  "version": "1.0",
  "description": "A simple Chrome extension that analyzes EULA.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/contentBundle.js"]
    }
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
